{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/products", "dest": "product.js" },
    { "src": "/category", "dest": "product_category.js" },
    { "src": "/products/sort", "dest": "product_category.js" },
    { "src": "/user", "dest": "user.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
